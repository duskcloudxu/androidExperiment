<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.myapplication.networkAccessing">

    <EditText
        android:id="@+id/networkAccessing_cityName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="49dp"
        android:hint="请输入城市"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/networkAccessing_userName"
        android:layout_width="143dp"
        android:layout_height="41dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="userName"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="@+id/networkAccessing_listView"
        app:layout_constraintTop_toBottomOf="@+id/networkAccessing_cityName" />

    <EditText
        android:id="@+id/networkAccessing_password"
        android:hint="password"
        android:layout_width="143dp"
        android:layout_height="39dp"
        android:layout_marginLeft="-9dp"
        android:layout_marginTop="9dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="@+id/networkAccessing_userName"
        app:layout_constraintTop_toBottomOf="@+id/networkAccessing_userName" />

    <Button
        android:id="@+id/networkAccessing_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="55dp"
        android:layout_marginRight="8dp"
        android:text="Login"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/networkAccessing_userName"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="115dp" />

    <ListView
        android:id="@+id/networkAccessing_listView"
        android:layout_width="361dp"
        android:layout_height="347dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="112dp"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/networkAccessing_cityName" />
</android.support.constraint.ConstraintLayout>
