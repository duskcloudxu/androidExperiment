        </activity>
        <activity android:name=".experiment_2_2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".experiment_3" />
        <activity android:name=".WeChatSimulation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
